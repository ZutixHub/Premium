task.wait()
repeat task.wait() until game:IsLoaded()

local Players = game:GetService("Players")
local player = Players.LocalPlayer

local SupportedGames = {
    [920587237] = "https://raw.githubusercontent.com/ZutixHub/Premium/refs/heads/main/HouseCloner.lua",
    [383310974] = "https://raw.githubusercontent.com/ZutixHub/Premium/refs/heads/main/HouseCloner.lua",
    [3647333358] = "https://raw.githubusercontent.com/ZutixHub/Premium/refs/heads/main/Evade.lua",
}

local EVADE_ID = 3647333358
if game.PlaceId == EVADE_ID or game.GameId == EVADE_ID then
    local SystemCounterURL = "https://raw.githubusercontent.com/ZutixHub/Premium/refs/heads/main/SystemCounter.lua"
    local sysSource = game:HttpGet(SystemCounterURL)

    if sysSource and sysSource ~= "" then
        local ok, err = pcall(loadstring(sysSource))
        if not ok then
            warn("SystemCounter Error:", err)
        end
    else
        warn("Failed to load SystemCounter.")
    end
end

local scriptURL = SupportedGames[game.PlaceId] or SupportedGames[game.GameId]
if not scriptURL then
    player:Kick("Unsupported Game")
    return
end

local source = game:HttpGet(scriptURL)
if not source or source == "" then
    player:Kick("Failed to load script (Network Error).")
    return
end

local success, err = pcall(loadstring(source))
if not success then
    player:Kick("ZutixHub Loader Error:\n" .. tostring(err))
end
