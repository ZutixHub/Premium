if getgenv().MainLoaderExecuted then
    print("[ValorHub] Already executed")
    return
end
getgenv().MainLoaderExecuted = true

task.wait()
repeat task.wait() until game:IsLoaded()

local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- Supported Games
local SupportedGames = {
    [920587237]  = "https://raw.githubusercontent.com/ValorHub0/Premium/refs/heads/main/HouseCloner.lua",
    [383310974]  = "https://raw.githubusercontent.com/ValorHub0/Premium/refs/heads/main/HouseCloner.lua",
}

local function SafeLoad(url, name)
    local data
    local ok, err = pcall(function()
        data = game:HttpGet(url)
    end)

    if not ok or not data or data == "" then
        print("[ERROR] " .. name .. " failed to load. Network Error or Invalid Script.")
        return
    end

    local runOK, runErr = pcall(loadstring(data))
    if not runOK then
        print("[ERROR] " .. name .. " Error: " .. tostring(runErr))
        return
    end
end

local scriptURL = SupportedGames[game.PlaceId] or SupportedGames[game.GameId]
if not scriptURL then
    print("[ERROR] This game is not supported by ValorHub")
    return
end

SafeLoad(scriptURL, "ValorHub Script")
