local games = {
    [920587237] = "https://raw.githubusercontent.com/ZutixHub/Premium/refs/heads/main/HouseCloner.lua",
    [8540168650] = "https://raw.githubusercontent.com/ZutixHub/Premium/refs/heads/main/DiamondFarm.lua" 
}

local gameId = game.PlaceId
local scriptURL = games[gameId]

if scriptURL then
    print("Loading script for: " .. game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name)
    loadstring(game:HttpGet(scriptURL))()
else
    game.Players.LocalPlayer:Kick("Unsupported game.")
end
