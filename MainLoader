task.wait() 
repeat task.wait() until game:IsLoaded()

local player = game:GetService("Players").LocalPlayer

local SupportedGames = {
    [920587237] = "https://raw.githubusercontent.com/ZutixHub/Premium/refs/heads/main/HouseCloner.lua",
    [383310974] = "https://raw.githubusercontent.com/ZutixHub/Premium/refs/heads/main/HouseCloner.lua",
    [7326934954] = "https://raw.githubusercontent.com/ZutixHub/Premium/refs/heads/main/DiamondFarm.lua"
}

local scriptURL = SupportedGames[game.PlaceId] or SupportedGames[game.GameId]
if not scriptURL then
    player:Kick("Unsupported Game")
    return
end

local source = game:HttpGet(scriptURL)
if not source or source == "" then
    player:Kick("Failed to load script (Network Error).")
    return
end

local success, err = pcall(loadstring(source))
if not success then
    player:Kick("ZutixHub Loader Error:\n" .. tostring(err))
end
